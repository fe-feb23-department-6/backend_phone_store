    "express": "^4.18.2"
  },
  "devDependencies": {
    "@mate-academy/eslint-config": "0.0.25",
    "@mate-academy/students-ts-config": "^0.0.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
